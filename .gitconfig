# git config --global core.editor code
# git config --global --edit
[user]
	email = jordyfaria0@gmail.com
	name = AjJordy
[core]
	editor = code --wait
[alias]
	c = !sh -c 'git add --all && git commit -m $1' -
	s = !git status -s	
	l = !git log --pretty=format:'%C(blue)%h%C(red)%d %C(white)%s - %C(cyan)%cn, %C(gren)%cr'
	t = !sh -c 'git tag -a $1 -m $1' 
	amend = !git add --all && git commit --amend --no-edit
[push]
	followTags = true
